[
    {
        "group_name":"register_only",
        "weight": 1,
        "work_flow":[
            {
                "task":"register",
                "lifetime": "$2"
            },
            {
                "task":"sleep",
                "interval": 30
            },
            {
                "task":"deregister"
            }
        ]
    },
    {
        "group_name":"notify",
        "weight": 1,
        "work_flow":[
            {
                "task":"register",
                "ep": "$1",
                "lifetime":120,
                "object_links": "</>;rt=\"oma.lwm2m\",</3/0>,</19/0>"
            },
            {
                "task":"wait_observe",
                "path":"/3/0",
                "timeout": 5
            },
            {
                "task":"wait_observe",
                "path":"19/0/0",
                "body": "abcde",
                "timeout": 5
            },
            {
                "task":"notify",
                "path":"19/0/0",
                "body": {
                    "type": "auto_gen_binary",
                    "size": 12
                }
            },
            {
                "task":"sleep",
                "interval": 30
            },
            {
                "task":"deregister"
            }
        ]
    }
]
